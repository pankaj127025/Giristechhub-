package CollectionApplication;

import java.util.Vector;

public class AscendingApp {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
			Vector v = new Vector();
			v.add(5);
			v.add(7);
			v.add(8);
			v.add(3);
			v.add(13);

for(int i=0;i<v.size();i++)
{
	for(int j=i+1;j<v.size();j++)
	{
		
		Object prev=v.get(i);
	   Object next=v.get(j);
if((int)prev>(int)next)
{
	v.set(j, prev);
	v.set(i, next);
}

	
	}
	
}
for(int i=0;i<v.size();i++)
{
	
	Object o=v.get(i);
	System.out.println(o);
}	


			
			System.out.println("\n Max value in collection is \t" + max);
		}

	

}
