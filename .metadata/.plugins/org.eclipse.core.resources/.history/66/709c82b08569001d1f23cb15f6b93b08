package org.techhub;
class square{
	int no;
	void setvalue(int x)
	{
		no=x;
	}
	void showsquare()
	{
		System.out.printf("square is %d of number %d",no*no,no);
	}
}
public class FirstApp {
	public static void main(String args[])
	{
		
	
		square s1=new square();
		square s2=s1;
		s2.setvalue(50);
		s1.showsquare();	
	}

}
