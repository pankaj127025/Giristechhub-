package practical0812;

public class ptwo {

	public static void main(String[] args) {
	

			
			for(int i=1;i<=9;i++)
			{
				for(int j=1;j<=5;j++)
				{
					if (j<=i && j<=11-i)
					{
						System.out.print("*");
					}
					else
					{
						System.out.print(" ");
					}
					
				}
				System.out.println("\n");
			}
		// TODO Auto-generated method stub

	}

}
